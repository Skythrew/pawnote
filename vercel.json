{
  "version": 2,
  "public": false,

  "github": {
    "enabled": false,
    "silent": true
  },

  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ],
  
  "build": {
    "env": {
      "ENABLE_VC_BUILD": "1"
    }
  }
}
