{
  "name": "pornote",
  "description": "Re-design de l'application web Pronote. Celle-ci est complètement personnalisable et inclut de nouvelles fonctionnalités.",
  "homepage": "https://pornote.vercel.app",
  "version": "0.1.1",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "solid-start dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "build": "pnpm run lint && solid-start build",
    "release": "pnpm run lint && release-it"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.9",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.4.2",
    "got": "^12.3.1",
    "haversine-distance": "^1.2.1",
    "ky": "^0.31.1",
    "localforage": "^1.10.0",
    "node-forge": "^1.3.1",
    "pako": "^2.0.4",
    "solid-js": "^1.5.1"
  },
  "devDependencies": {
    "@types/node-forge": "^1.0.4",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-plugin-solid": "^0.7.1",
    "release-it": "^15.4.0",
    "solid-start": "0.1.0-alpha.99",
    "solid-start-vercel": "0.1.0-alpha.99",
    "typescript": "4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "vite": "^3.0.9",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-windicss": "^1.8.7",
    "windicss": "^3.5.6",
    "workbox-window": "^6.5.4"
  },
  "author": {
    "url": "https://github.com/Vexcited",
    "email": "mikkel@milescode.dev",
    "name": "Mikkel RINGAUD"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm run lint"
      ]
    },
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagAnnotation": "Release v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  }
}
