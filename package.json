{
  "name": "pornote",
  "description": "Client web non-officiel Pronote.",
  "homepage": "https://www.pornote.ml",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "solid-start dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "build": "pnpm run lint && solid-start build",
    "release": "pnpm run lint && release-it"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.5.0",
    "dayjs": "^1.11.6",
    "got": "^12.5.3",
    "haversine-distance": "^1.2.1",
    "html-entities": "^2.3.3",
    "localforage": "^1.10.0",
    "node-forge": "^1.3.1",
    "pako": "^2.1.0",
    "set-cookie-parser": "^2.5.1",
    "solid-headless": "^0.13.0",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.5"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.34",
    "@types/node-forge": "^1.3.0",
    "@types/pako": "^2.0.0",
    "@types/set-cookie-parser": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-solid": "^0.7.4",
    "release-it": "^15.5.0",
    "solid-start-vercel": "0.2.5",
    "typescript": "4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.13",
    "vite": "^3.2.4",
    "vite-plugin-pwa": "^0.13.3",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6",
    "workbox-window": "^6.5.4"
  },
  "author": {
    "url": "https://github.com/Vexcited",
    "email": "mikkel@milescode.dev",
    "name": "Mikkel RINGAUD"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm run lint"
      ]
    },
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagAnnotation": "Release v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  }
}
